.row

  #fs-carousel.carousel.slide{"data-ride" => "carousel"}

    = carousel_image_html

    / Controls
    %a.left.carousel-control{"data-slide" => "prev", :href => "#fs-carousel", :role => "button"}
      %span.glyphicon.glyphicon-chevron-left{"aria-hidden" => "true"}
      %span.sr-only Previous
    %a.right.carousel-control{"data-slide" => "next", :href => "#fs-carousel", :role => "button"}
      %span.glyphicon.glyphicon-chevron-right{"aria-hidden" => "true"}
      %span.sr-only Next

.row
  %h3.col-xs-3.col-xs-offset-3
    %span.glyphicon.glyphicon-arrow-up
    Animated Hero Slideshow
  %h3.col-xs-3
    Image Grid
    %span.glyphicon.glyphicon-arrow-down
